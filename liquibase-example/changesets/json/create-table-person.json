{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "create-person-table-json",
        "author": "mezentseva.pa@edu.spbstu.ru",   
        "changes": [     
          {     
            "createTable": {
              "tableName": "person_json",
              "columns": [
                {
                  "column": {
                    "name": "id",
                    "type": "serial",
                    "constraints": {
                      "primaryKey": "true",
                      "nullable": "false"
                     } 
                  }
                },
                {
                  "column": {
                    "name": "name",
                    "type": "varchar(50)",
                    "constraints": {
                      "nullable": "false"
                    }
                  }
                },
                {
                  "column": {
                    "name": "city",
                    "type": "varchar(50)"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}